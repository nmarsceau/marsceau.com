{
    "env": {
        "browser": true,
		"es6": true
    },
    "rules": {
        "func-names": ["error", "never"],
        "prefer-arrow-callback": 0,
        "space-before-function-paren": 0,
        "block-spacing": 0,
        "brace-style": 0,
        "no-param-reassign": 0,
        "no-plusplus": 0,
        "prefer-template": 0,
        "default-case": 0,
        "linebreak-style": 0,
		"quotes": ["error", "double"],
		"indent": [1, "tab"]
    }
}
